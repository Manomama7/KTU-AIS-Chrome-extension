{
  "manifest_version": 2,

  "short_name": "KTU AIS",
  "name": "KTU AIS Chrome extension",
  "version": "0.0.1",

  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },

  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}